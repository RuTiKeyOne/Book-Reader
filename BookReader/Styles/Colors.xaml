<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="WhiteBaseBrush" Color="#F1F1F1" />
    <Color x:Key="WhiteBaseColor">#F1F1F1</Color>

    <SolidColorBrush x:Key="DarkBaseBrush" Color="#252526" />
    <Color x:Key="DarkBaseColor">#252526</Color>

    <SolidColorBrush x:Key="RedBaseBrush" Color="#E01020" />
    <Color x:Key="RedBaseColor">#E01020</Color>

    <SolidColorBrush x:Key="LightRedBaseBrush" Color="#E91728" />
    <Color x:Key="LightRedBaseColor">#E91728</Color>

    <SolidColorBrush x:Key="BlueBaseBrush" Color="#007ACD" />
    <Color x:Key="BlueRedBaseColor">#007ACD</Color>

    <SolidColorBrush x:Key="LightDarkBaseBrush" Color="#333337" />
    <Color x:Key="LightDarkBaseColor">#333337</Color>

    <SolidColorBrush x:Key="GreyBaseBrush" Color="#3F3F46" />
    <Color x:Key="GreyDarkBaseColor">#3F3F46</Color>

    <SolidColorBrush x:Key="LightGreyBaseBrush" Color="#575761" />
    <Color x:Key="LightGreyDarkBaseColor">#575761</Color>

    <SolidColorBrush x:Key="LightBlackBaseBrush" Color="#3F3F3F"/>
    <Color x:Key="LightBlackBaseColor">#3F3F3F</Color>

    <SolidColorBrush x:Key="DarkWhiteBaseBrush" Color="#A0A2AC"/>
    <Color x:Key="DarkWhiteBaseColor">#A0A2AC</Color>

</ResourceDictionary>