<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Style x:Key="HightTextBlock" TargetType="TextBlock">
        <Setter Property="FontSize" Value="{DynamicResource TheBiggers}"/>
        <Setter Property="Foreground" Value="{DynamicResource WhiteBaseBrush}"/>
        <Setter Property="FontWeight" Value="Light"/>
    </Style>
    
</ResourceDictionary>